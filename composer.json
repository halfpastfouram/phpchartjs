{
  "name": "halfpastfouram/phpchartjs",
  "description": "PHP library for ChartJS 2.0",
  "keywords": [
    "chartjs",
    "graph",
    "php"
  ],
  "homepage": "http://github.com/halfpastfouram/PHPChartJS",
  "type": "package",
  "version": "v1.1.0",
  "license": "AGPL-3.0-or-later",
  "authors": [
    {
      "name": "Bob Kruithof"
    }
  ],
  "require": {
    "php": ">=5.6.0 || ^7.0",
    "zendframework/zend-json": "3.0.0",
    "zf1/zend-reflection": "1.12.11",
    "halfpastfouram/collection": "1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*",
    "codeclimate/php-test-reporter": "dev-master",
    "squizlabs/php_codesniffer": "3.0.0",
    "friendsofphp/php-cs-fixer": "2.11.1"
  },
  "autoload": {
    "psr-4": {
      "Halfpastfour\\PHPChartJS\\": "src/",
      "Test\\": "test/"
    }
  },
  "scripts": {
    "cs-check": "./vendor/bin/phpcs",
    "cs-fix": "./vendor/bin/phpcbf"
  }
}
